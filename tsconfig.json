{
  "include": [
      "./src/**/*"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "types": [
      "jest",
      "puppeteer",
      "jest-environment-puppeteer",
    ],
    "outDir": "dist",
    "jsx": "react-jsx"
  },
}

